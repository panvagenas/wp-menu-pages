{
    "name": "panvagenas/wp-menu-pages",
    "description": "A library for creating settings pages for WordPress",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Panagiotis Vagenas",
            "email": "pan.vagenas@gmail.com",
            "homepage": "https://gr.linkedin.com/in/panvagenas",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "respect/validation": "^1",
        "twig/twig": "^1"
    },
    "require-dev": {
        "satooshi/php-coveralls": ">=0.6"
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "files": [],
        "psr-4": {
            "Pan\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pan\\QueryBuilder\\Tests\\": "tests"
        }
    },
    "archive": {
        "exclude": [
            "docs/*",
            "tests/*"
        ]
    }

}